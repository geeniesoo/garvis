{
  "name": "garvis",
  "version": "1.0.0",
  "description": "A Slack-based AI assistant that spawns specialized agents for different tasks",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
